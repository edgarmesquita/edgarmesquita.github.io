{
    "name": "edgarmesquita",
    "version": "1.0.0",
    "homepage": "https://edgarmesquita.com",
    "scripts": {
        "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss ./public -o ./src/css",
        "build-js": "react-scripts-ts build",
        "build-ts": "tsc",
        "build": "npm-run-all copy build-css build-js",
        "predeploy": "npm run build",
        "start-js": "react-scripts-ts start",
        "start": "npm-run-all -p copy watch-css start-js",
        "deploy": "gh-pages -b master -d build",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss ./public -o ./src/css --watch --recursive",
        "copy": "cp -R node_modules/font-awesome/fonts ./src/"
    },
    "dependencies": {
        "bootstrap": "^4.1.3",
        "classnames": "^2.2.6",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "font-awesome": "^4.7.0",
        "react": "^16.5.2",
        "react-dom": "^16.5.2",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.3.1",
        "react-waterfall": "^3.0.7",
        "reactstrap": "^6.5.0"
    },
    "devDependencies": {
        "@types/jest": "^22.2.3",
        "@types/lodash": "^4.14.113",
        "@types/node": "^10.11.3",
        "@types/react": "^16.4.14",
        "@types/react-dom": "^16.0.8",
        "@types/react-router": "^4.0.31",
        "@types/react-router-dom": "^4.3.1",
        "@types/reactstrap": "^6.4.0",
        "babel-jest": "^23.6.0",
        "gh-pages": "^2.0.0",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "react-scripts-ts": "3.1.0",
        "typescript": "^3.1.1"
    }
}
